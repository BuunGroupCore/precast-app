    "@sveltejs/adapter-static": "^3.0.5",
    "svelte-check": "^4.0.4",
    "typescript": "^5.6.3"{{/if}}{{#if (eq styling "tailwind")}},
    "tailwindcss": "^3.4.13",
    "postcss": "^8.4.47",
    "autoprefixer": "^10.4.20"{{/if}}{{#if (eq styling "scss")}},
    "sass": "^1.79.4"{{/if}}{{#if (eq orm "prisma")}},
    "prisma": "^5.20.0"{{/if}}
  },
  "dependencies": {
    {{#ifAny (eq backend "express") (eq backend "fastify") (eq backend "hono")}}
    "axios": "^1.7.7"{{#if (eq orm "prisma")}},{{/if}}{{/ifAny}}{{#if (eq orm "prisma")}}
