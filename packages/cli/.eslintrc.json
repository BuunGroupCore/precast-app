{
  "extends": "../../.eslintrc.json",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "node": true
  },
  "rules": {
    "no-console": "off",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "ignorePatterns": [
    "dist/**",
    "build/**",
    "node_modules/**",
    "src/templates/**",
    "*.config.js",
    "*.config.cjs",
    "*.config.mjs",
    "generated-tests/**"
  ]
}
