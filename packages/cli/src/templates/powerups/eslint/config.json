{
  "id": "eslint",
  "name": "ESLint",
  "description": "Find and fix problems in your JavaScript code",
  "dependencies": {},
  "devDependencies": {
    "react": [
      "eslint@^9.19.0",
      "@eslint/js@^9.19.0",
      "eslint-plugin-react@^7.37.0",
      "eslint-plugin-react-hooks@^5.0.0",
      "eslint-plugin-react-refresh@^0.5.0"
    ],
    "next": ["eslint@^9.19.0", "@eslint/js@^9.19.0", "@next/eslint-config-next"],
    "vue": [
      "eslint@^9.19.0",
      "@eslint/js@^9.19.0",
      "eslint-plugin-vue@^9.31.0",
      "@vue/eslint-config-typescript@^14.1.0"
    ],
    "angular": [
      "@angular-eslint/eslint-plugin@^18.4.0",
      "@angular-eslint/template-parser@^18.4.0",
      "eslint@^9.19.0",
      "@eslint/js@^9.19.0"
    ],
    "svelte": ["eslint@^9.19.0", "@eslint/js@^9.19.0", "eslint-plugin-svelte@^2.46.0"],
    "solid": ["eslint@^9.19.0", "@eslint/js@^9.19.0", "eslint-plugin-solid@^0.15.0"],
    "*": [
      "eslint@^9.19.0",
      "@eslint/js@^9.19.0",
      "@typescript-eslint/parser@^8.19.0",
      "@typescript-eslint/eslint-plugin@^8.19.0"
    ]
  },
  "envVariables": {},
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "setupFiles": {
    "*": [
      { "template": "eslint.config.js.hbs", "output": "eslint.config.js" },
      { "template": ".eslintignore.hbs", "output": ".eslintignore" }
    ]
  },
  "packageJsonConfig": {}
}
