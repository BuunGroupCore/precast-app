{
  "name": "test-stripe",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "cd apps/api && prisma generate",
    "predev": "cd apps/api && prisma generate && prisma db push --skip-generate",
    "prebuild": "cd apps/api && prisma generate",
    "db:generate": "cd apps/api && prisma generate",
    "db:push": "cd apps/api && prisma db push",
    "db:migrate": "cd apps/api && prisma migrate dev",
    "db:studio": "cd apps/api && prisma studio",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "turbo": "^2.5.5",
    "typescript": "^5.4.2",
    "eslint": "^8.57.0"
  },
  "packageManager": "bun@1.2.20",
  "overrides": {}
}
