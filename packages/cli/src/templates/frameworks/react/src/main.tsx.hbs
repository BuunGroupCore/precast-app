import React from 'react'
import ReactDOM from 'react-dom/client'
import App from './App{{#if typescript}}.tsx{{else}}.jsx{{/if}}'
{{#if (or (eq styling "css") (eq styling "tailwind"))}}import './styles/globals.css'{{/if}}
{{#if (eq styling "scss")}}import './styles/globals.scss'{{/if}}
{{#if (eq apiClient "tanstack-query")}}
import { QueryProvider } from './providers/query-provider'
{{/if}}
{{#if (eq apiClient "swr")}}
import { SWRProvider } from './providers/swr-provider'
{{/if}}

ReactDOM.createRoot(document.getElementById('root'){{#if typescript}}!{{/if}}).render(
  <React.StrictMode>
    {{#if (eq apiClient "tanstack-query")}}
    <QueryProvider>
      <App />
    </QueryProvider>
    {{else if (eq apiClient "swr")}}
    <SWRProvider>
      <App />
    </SWRProvider>
    {{else}}
    <App />
    {{/if}}
  </React.StrictMode>,
)