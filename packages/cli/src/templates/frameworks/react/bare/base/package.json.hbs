{
  "name": "{{name}}-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "{{#if typescript}}tsc -b && {{/if}}vite build",
    "preview": "vite preview"{{#if typescript}},
    "type-check": "tsc --noEmit",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"{{/if}}{{#if (eq orm "prisma")}},
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"{{/if}}{{#if (eq orm "mongoose")}},
    "db:seed": "node scripts/seed.js"{{/if}}
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"{{#if typescript}},
    "lucide-react": "^0.263.1",
    "react-icons": "^5.0.1"{{/if}}{{#if (eq styling "styled-components")}},
    "styled-components": "^6.1.13"{{/if}}{{#ifAny (eq backend "express") (eq backend "fastify") (eq backend "hono")}},
    "axios": "^1.7.7",
    "@tanstack/react-query": "^5.59.0"{{/ifAny}}{{#if (eq orm "prisma")}},
    "@prisma/client": "^6.14.0"{{/if}}{{#if (eq orm "mongoose")}},
    "mongoose": "^8.8.3"{{/if}}
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "vite": "^5.4.0"{{#if typescript}},
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/node": "^20.14.0",
    "typescript": "^5.7.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5"{{/if}}{{#if (eq styling "tailwind")}},
    "tailwindcss": "^3.4.13",
    "postcss": "^8.4.47",
    "autoprefixer": "^10.4.20"{{/if}}{{#if (eq styling "scss")}},
    "sass": "^1.79.4"{{/if}}{{#if (and typescript (eq styling "styled-components"))}},
    "@types/styled-components": "^5.1.34"{{/if}}{{#if (eq orm "prisma")}},
    "prisma": "^6.14.0"{{/if}}
  }
}